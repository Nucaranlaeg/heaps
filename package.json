{
  "scripts": {
    "sass": "sass src/style/:dist/style/",
    "sass-w": "sass src/style/:dist/style/ --watch",
    "ts": "tsc",
    "ts-w": "tsc --watch",
    "build": "npm run sass && npm run ts",
    "watch": "npm-run-all --parallel sass-w ts-w"
  },
  "devDependencies": {
    "minify-ts": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "typescript": "^5.0.4"
  }
}
